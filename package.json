{
  "name": "pado_visualize",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "scripts": {
    "watch": "parcel watch --public-url /static -d pado_visualize/static pado_visualize/static_src/*.js",
    "build": "parcel build --public-url /static -d pado_visualize/static pado_visualize/static_src/*.js"
  },
  "dependencies": {
    "images": "^3.2.3",
    "normalize.css": "^8.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "openseadragon": "^2.4.2",
    "parcel-bundler": "^1.12.5",
    "parcel-plugin-static-files-copy": "^2.6.0",
    "parcel-plugin-structurize": "^2.4.0",
    "prettier": "^2.2.1",
    "sass": "^1.32.8"
  },
  "staticFiles": {
    "staticPath": "pado_visualize/static_src",
    "watcherGlob": "images/*.jpg"
  },
  "parcel-plugin-structurize": {
    "rules": [
      {
        "match": "*_{grouphover,hover,pressed,rest}*.png",
        "folder": "images/openseadragon"
      },
      {
        "match": "*.{jpg,png}",
        "folder": "images"
      },
      {
        "match": "fa-*.{eot,svg,ttf,woff,woff2}",
        "folder": "."
      },
      {
        "match": "*.{css,css.map}",
        "folder": "css"
      },
      {
        "match": "*.{js,js.map}",
        "folder": "js"
      }
    ],
    "verbose": true
  }
}
